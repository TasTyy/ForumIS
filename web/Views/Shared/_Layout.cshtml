<header class="s-header">


            <div class="row s-header__nav-wrap">
                <nav class="s-header__nav">
                    <ul>
                        <li class="current">
                            <a class="smoothscroll" asp-area="" asp-controller="Home" asp-action="Index">Domov</a>
                        </li>
                        <li>
                            <a class="smoothscroll" asp-area="" asp-controller="Posts" asp-action="Index">Objave</a>
                        </li>
                        <li>
                            <a class="smoothscroll" asp-area="" asp-controller="Categories" asp-action="Index">Kategorije</a>
                        </li>
                    </ul>
                    <partial class="smoothscroll" name="_LoginPartial" />
                </nav>
            </div> <!-- end s-header__nav-wrap -->

        </header> <!-- end s-header -->

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - Koren & Suhovršnik - <a asp-area="" asp-controller="Home" asp-action="Privacy">Zasebnost</a>
        </div>
    </footer>
